{
  "name": "react-css-stagger",
  "version": "0.0.6",
  "description": "A component that adds CSS transitions to create a staggered animation on entrance",
  "main": "dist/index.js",
  "scripts": {
    "build": "npm run clean && webpack",
    "clean": "rimraf ./dist",
    "test": "jest --env=jsdom",
    "jest": "jest --env=jsdom --watch"
  },
  "jest": {
    "testRegex": "Spec\\.js$",
    "rootDir": "src",
    "setupFiles": ["../jest.setup.js"]
  },
  "keywords": [
    "react",
    "css",
    "transitions",
    "stagger",
    "staggered",
    "animation",
    "ngAnimate",
    "centricient",
    "quiq"
  ],
  "author": "natenorberg",
  "repository": "https://github.com/natenorberg/react-css-stagger",
  "license": "MIT",
  "devDependencies": {
    "babel-core": "^6.9.1",
    "babel-eslint": "^6.0.4",
    "babel-jest": "21.0.2",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-0": "^6.5.0",
    "enzyme": "3.1.0",
    "enzyme-adapter-react-16": "1.0.2",
    "enzyme-to-json": "3.1.4",
    "es6-shim": "^0.35.1",
    "eslint": "1.10.3",
    "eslint-plugin-react": "3.16.1",
    "jasmine": "^2.4.1",
    "jest": "21.0.2",
    "react": "^16.0.0",
    "react-addons-test-utils": "^15.6.2",
    "react-dom": "^16.0.0",
    "react-test-renderer": "^15.5.4",
    "rimraf": "^2.5.2",
    "webpack": "^1.13.1"
  },
  "dependencies": {
    "prop-types": "^15.5.10"
  },
  "peerDependencies": {
    "prop-types": ">=15",
    "react": ">=15"
  }
}
